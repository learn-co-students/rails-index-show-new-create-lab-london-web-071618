<p id="notice"><%= notice %></p>

<h1>Listing Coupons</h1>

<table>
  <thead>
    <tr>
      <th>Store</th>
      <th>Coupon Code</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @coupons.each do |coupon| %>
    <tr>
        <td><%= coupon.store %></td>
        <td><%= link_to coupon.coupon_code, coupon_path(coupon) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
